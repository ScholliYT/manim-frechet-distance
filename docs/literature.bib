	file = {Buchin et al. - 2017 - Four Soviets Walk the Dog-Improved Bounds for Comp.pdf:/home/tom/Zotero/storage/NLX63EAS/Buchin et al. - 2017 - Four Soviets Walk the Dog-Improved Bounds for Comp.pdf:application/pdf},
