	doi = {10.1109/IAEAC54830.2022.9929549},
	abstract = {In order to further improve the accuracy of gesture authentication, based on the traditional Frechet distance and Support Vector Machine (SVM), this paper proposes a two-level authentication algorithm that combines space trajectory similarity and feature classification, which is called the F-S algorithm. The method mainly uses the linear accelerometer data of the mobile phone to obtain the three-dimensional spatial trajectory of the gesture action in the mobile phone coordinate system after preprocessing, and then extracts the time domain features of the trajectory, and then uses the F-S secondary authentication algorithm to identify and authenticate the gesture. The experimental results showed that the false rejection rate FRR of the F-S algorithm in the gesture recognition process is 4.13\%, and the false acceptance rate FAR is 1.26\%.},
	booktitle = {2022 {IEEE} 6th {Advanced} {Information} {Technology}, {Electronic} and {Automation} {Control} {Conference} ({IAEAC} )},
